{
  "name": "langgraph-basic-dockerfile",
  "version": "1.0.0",
  "description": "Minimal LangGraph JS agent for AWS Bedrock AgentCore",
  "type": "module",
  "main": "agent.js",
  "scripts": {
    "start": "node agent.js",
    "dev": "sls agentcore dev",
    "deploy": "sls deploy",
    "invoke": "sls invoke agent --agent assistant",
    "logs": "sls agentcore logs --agent assistant --tail",
    "remove": "sls remove"
  },
  "keywords": [
    "langgraph",
    "langchain",
    "bedrock",
    "agentcore",
    "serverless",
    "ai-agent"
  ],
  "dependencies": {
    "@langchain/aws": "^0.1.0",
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "bedrock-agentcore": "^0.2.0",
    "zod": "^3.23.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
